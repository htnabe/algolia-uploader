{
  "name": "algolia-uploader",
  "version": "0.0.9",
  "description": "command-line util to upload Algolia source",
  "repository": {
    "type": "git",
    "url": "https://github.com/htnabe/algolia-uploader.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "bin": "./dist/index.mjs",
  "files": [
    "dist"
  ],
  "dependencies": {
    "algoliasearch": "^5.40.1",
    "citty": "^0.1.6",
    "es-toolkit": "^1.40.0"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.51.0",
    "@types/node": "^24.8.1",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vitest": "^4.0.14"
  },
  "scripts": {
    "build": "unbuild",
    "prepack": "unbuild",
    "prepare": "node .husky/install.mjs",
    "lint": "prettier . --write",
    "test": "vitest run",
    "dev": "dotenvx run jiti ./src/index.ts"
  },
  "keywords": [
    "algolia"
  ],
  "author": "htnabe <https://github.com/htnabe>",
  "license": "ISC",
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
