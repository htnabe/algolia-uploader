{
  "name": "algolia-uploader",
  "version": "0.0.3",
  "description": "command-line util to upload Algolia source",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "bin": "./dist/index.mjs",
  "files": [
    "dist"
  ],
  "dependencies": {
    "algoliasearch": "^5.20.3",
    "citty": "^0.1.6",
    "es-toolkit": "^1.32.0"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.38.3",
    "@types/node": "^22.13.5",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.2",
    "typescript": "^5.7.3",
    "unbuild": "^3.5.0",
    "vitest": "^3.0.7"
  },
  "scripts": {
    "build": "unbuild",
    "prepack": "unbuild",
    "prepare": "node .husky/install.mjs",
    "lint": "yarn prettier . --write",
    "test": "vitest run",
    "dev": "dotenvx run jiti ./src/index.ts"
  },
  "keywords": [
    "algolia"
  ],
  "author": "htnabe <https://github.com/htnabe>",
  "license": "ISC",
  "packageManager": "yarn@4.6.0",
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
